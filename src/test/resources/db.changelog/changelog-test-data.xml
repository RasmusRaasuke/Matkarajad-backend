<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
       http://www.liquibase.org/xml/ns/dbchangelog
       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <!-- Include your main changelog file -->
    <include file="/db/changelog/changelog-master.xml"/>

    <changeSet id="mock-data-user" author="dev">
        <insert tableName="user_entity">
            <column name="id" valueNumeric="1"/>
            <column name="username" value="user"/>
        </insert>
    </changeSet>

    <changeSet id="mock-data-route" author="dev">
        <insert tableName="camping_route_entity">
            <column name="id" valueNumeric="1"/>
            <column name="user_id" valueNumeric="1"/>
            <column name="name" value="Test Route"/>
        </insert>
    </changeSet>

</databaseChangeLog>
